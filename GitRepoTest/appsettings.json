{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Serilog": {
        "Using": [ "Serolog.Sinks.File" ],
        "MinimumLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "System": "Warning"
        },
        "WriteTo": [
            {
                "Name": "File",
                "Arg": {
                    "path": "/Test.txt",
                    "template": "{TimseStamp : -o} {level: u3} {RequestId} {message}{NewLine}{Exception}",
                    "rollInterval": "",
                    "rollOnFileSizeLimit": true,
                    "rollFileCountLimit": null,
                    "shared": true,
                }
            }
        ]

    },
  "AllowedHosts": "*"
}
